{
    "name": "Airbnb Price Estimator",
    "description": "Estimate the price per night of an AirBnb room.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "airbnb-logo.png"
    },
    "icons":{
      "128": "airbnb-logo.png"
    }
    
  }